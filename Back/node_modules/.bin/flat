#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/henry/PI-Videogames/Back/node_modules/flat/node_modules:/mnt/d/henry/PI-Videogames/Back/node_modules:/mnt/d/henry/PI-Videogames/node_modules:/mnt/d/henry/node_modules:/mnt/d/node_modules:/mnt/d/henry/PI-Videogames/Back/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/henry/PI-Videogames/Back/node_modules/flat/node_modules:/mnt/d/henry/PI-Videogames/Back/node_modules:/mnt/d/henry/PI-Videogames/node_modules:/mnt/d/henry/node_modules:/mnt/d/node_modules:/mnt/d/henry/PI-Videogames/Back/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../flat/cli.js" "$@"
else
  exec node  "$basedir/../flat/cli.js" "$@"
fi
